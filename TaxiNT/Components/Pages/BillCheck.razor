@page "/checkers/{userId}"
@page "/checkers/{userId}/{date}"
@layout EmptyMainLayout

<SeoHeaderComponent Title="Phiếu Checker v4.1 - Taxi Nam Thắng"
                    Description="@($"Tài xế {userId} - {date}")" Ads="false" />

<BillCheckComponent userId="@userId" date="@date" @rendermode="new InteractiveWebAssemblyRenderMode(prerender: true)" />

@code {
    [Parameter]
    public string userId { get; set; } = string.Empty;
    [Parameter]
    public string? date { get; set; }
}
