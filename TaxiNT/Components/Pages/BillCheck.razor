@page "/checkers/{userId}"
@page "/checkers/{userId}/{date}"
@layout MainLayoutEmpty

<!-- GG Adsense -->
<meta name="google-adsense-account" content="ca-pub-4772558027240339">
<script async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js?client=ca-pub-4772558027240339"
        crossorigin="anonymous"></script>

<!--AMP Adsense-->
<script async custom-element="amp-ad" src="https://cdn.ampproject.org/v0/amp-ad-0.1.js"></script>
<script async custom-element="amp-auto-ads"
        src="https://cdn.ampproject.org/v0/amp-auto-ads-0.1.js">
